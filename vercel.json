{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "docs",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "GITHUB_PAGES": "0"
  }
}
